<!DOCTYPE html>
<html lang="en">
  <%- include("./partials/head.ejs") %>
  <body>
    <%- include("./partials/nav.ejs") %>
    <h1>Posts</h1>
    <ol>
      <% posts.forEach((post) => { %>
      <li>
        <a href="/post/<%= post.id %>"> 
          <%= post.title %>
        </a>
      </li>
      <% }) %>
    </ol>
    <h2><a href="/add-post">Add new Post</a></h2>
  </body>
</html>
